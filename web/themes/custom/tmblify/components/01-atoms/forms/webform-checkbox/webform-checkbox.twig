{% if not attributes %}
<fieldset class="form-item form-item--checkbox">
  <legend class="h3">Options as Checkboxes</legend>
  <ul class="form-item--checkboxes kevins-checkbox-component">
{% endif %}
  {% for key, checkbox in checkboxes %}
    {% include "@atoms/forms/webform-checkbox/_webform-checkbox-item.twig" with {
      checkbox_item: webformcheckboxes.title,
      checkbox_item_checked: webformcheckboxes.checked,
    } %}
  {% endfor %}
{% if not attributes %}
  </ul>
</fieldset>
{% endif %}
